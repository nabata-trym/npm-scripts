{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "all": "npm run sass && npm run autoprefixer",
    "sass": "node-sass css/contents.scss css/contents.css --output-style compressed",
    "autoprefixer": "postcss css/contents.css --use autoprefixer --replace",
    "watch": "onchange css/contents.scss -- npm run all"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.5.0",
    "node-sass": "^4.11.0",
    "onchange": "^5.2.0",
    "postcss": "^7.0.14",
    "postcss-cli": "^6.1.2"
  },
  "browserslist": [
    "last 2 versions",
    "not ie < 11",
    "Android >= 4",
    "iOS >= 10"
  ]
}
